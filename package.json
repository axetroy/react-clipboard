{
  "name": "@axetroy/react-clipboard",
  "version": "1.0.0",
  "description": "clipboard wrapper component for react",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "dev": "rimraf lib && webpack --config webpack.config.prod.js --display-error-details --progress --colors --watch",
    "build": "rimraf lib && webpack --config webpack.config.prod.js --display-error-details --progress --colors",
    "build:doc": "npm run build && rimraf docs && webpack --config webpack.config.doc.js --display-error-details --progress --colors",
    "deploy": "gh-pages -d ./docs"
  },
  "keywords": [
    "react",
    "react-component",
    "clipboard"
  ],
  "author": "axetroy",
  "license": "MIT",
  "peerDependencies": {
    "clipboard": "^1.7.1",
    "react": "^16.0.0",
    "proptypes": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clipboard": "^2.0.0",
    "copy-webpack-plugin": "^4.3.0",
    "gh-pages": "^1.1.0",
    "proptypes": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
